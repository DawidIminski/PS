#include <stdio.h>
#include <stdlib.h>

int zadanie_6_2_11(int tab[][100], const int);
int zadanie_6_2_12(const int n, const int m, int tab[n][m]);

int main()
{
    int tab[2][1] = {{1}, {2}};

    return 0;
}


int zadanie_6_2_11(int tab[][100], const int a){
    int tab1[a][100];
    int sum = 0;
    for(int i = 0; i < a; i++){
        for(int j = 0; j < 100; j++){
            sum += tab1[i][j];
        }
    }
    return sum;
}
int zadanie_6_2_12(const int n, const int m, int tab[n][m]){
    int sum = 0;
    for(int i = 0; i < n; i++){
        for(int j = 0; j < m; j++){
            sum += tab[i][j];
        }
    }
    return sum;
}
